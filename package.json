{
  "name": "wp-boiler-plate",
  "version": "0.210302.1",
  "description": "boiler-plate to develop WP theme",
  "author": "KOBAYASHI atsushi",
  "homepage": "https://github.com/kobaatsu/wp-boiler-plate",
  "main": "gulpfile.js",
  "scripts": {
    "start": "docker-compose up -d && npx gulp",
    "plugin": "npx gulp installPlugins",
    "init": "npx gulp installPlugins && npx gulp init && npx gulp wpStyle",
    "up": "docker-compose up",
    "stop": "docker-compose stop",
    "down": "docker-compose down",
    "cli": "docker-compose run --rm cli bash",
    "dist": "npx gulp dist",
    "build": "npx gulp dist",
    "watch": "npx gulp",
    "lint": "eslint --fix src/js/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kobaatsu/wp-boiler-plate.git"
  },
  "devDependencies": {
